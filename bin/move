
#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

# Welcomes User
puts "Welcome to Tic Tac Toe!"

#Instanitiate Empty Board
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

#Request Input
puts "Where would you like to go?"

#Store User input
input = gets.strip

#Apply move
index = input_to_index(input)

move(board, index)

#Display Board
display_board(board)
